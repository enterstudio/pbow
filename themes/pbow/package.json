{
  "name": "CALOPPS",
  "description": "CALOPPS customized theme",
  "version": "1.0.1",
  "keywords": [
    "css",
    "less",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "homepage": "http://urbaninsight.com",
  "author": "Lehel @Urban insight, Inc.",
  "scripts": {
    "test": "grunt test"
  },
  "style": "/css/style.css",
  "less": "less/style.less",
  "main": "./dist/js/npm",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org:urban-insight/startertheme-d7.git"
  },
  "bugs": {
    "url": "https://bitbucket.org:urban-insight/startertheme-d7/issues"
  },
  "license": {
    "type": "Apache-2.0",
    "url": "http://opensource.org/licenses/MIT"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "autoprefixer-core": "^6.0.1",
    "grunt": "^0.4.5",
    "grunt-autoprefixer": "^3.0.3",
    "grunt-contrib-csslint": "^0.5.0",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-jshint": "^0.11.3",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-uglify": "^0.10.0",
    "grunt-contrib-watch": "^0.6.1",
    "load-grunt-tasks": "^3.3.0"
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "files": [
    "fonts",
    "grunt/*.js",
    "grunt/*.json",
    "js/*.js",
    "less/**/*.less",
    "Gruntfile.js",
    "LICENSE"
  ],
  "jspm": {
    "main": "js/bootstrap",
    "directories": {
      "example": "examples",
      "lib": "dist"
    },
    "shim": {
      "js/bootstrap": {
        "imports": "jquery",
        "exports": "$"
      }
    },
    "buildConfig": {
      "uglify": true
    }
  }
}
